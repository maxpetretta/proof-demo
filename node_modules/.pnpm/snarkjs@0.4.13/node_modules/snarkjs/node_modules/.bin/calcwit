#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maximilian.petretta/dev/proof-demo/node_modules/.pnpm/circom_runtime@0.1.17/node_modules/circom_runtime/node_modules:/Users/maximilian.petretta/dev/proof-demo/node_modules/.pnpm/circom_runtime@0.1.17/node_modules:/Users/maximilian.petretta/dev/proof-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maximilian.petretta/dev/proof-demo/node_modules/.pnpm/circom_runtime@0.1.17/node_modules/circom_runtime/node_modules:/Users/maximilian.petretta/dev/proof-demo/node_modules/.pnpm/circom_runtime@0.1.17/node_modules:/Users/maximilian.petretta/dev/proof-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../circom_runtime@0.1.17/node_modules/circom_runtime/calcwit.js" "$@"
else
  exec node  "$basedir/../../../../../circom_runtime@0.1.17/node_modules/circom_runtime/calcwit.js" "$@"
fi
